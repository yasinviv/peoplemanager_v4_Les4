@model IList<Person>

@{
    ViewData["Title"] = "People List";
}

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    
    @foreach (var person in Model)
    {
        <partial name="PersonRowPartial" model="person"/>
    }
    </tbody>
</table>